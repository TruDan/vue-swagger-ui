{
  "name": "swagger-ui-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "watch": "npm-watch",
    "autobarrel": "create-index --recursive --ignoreUnsafe --extensions js jsx vue ts tsx --outputFile index.js ./src"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "oas-normalize": "^3.0.3",
    "vue": "^2.6.11",
    "vue-json-viewer": "^2.2.19",
    "vue-markdown-render": "^1.1.2",
    "vue-router": "^3.2.0",
    "vuetify": "^2.4.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@trudan97/create-index": "^1.0.2",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "npm-watch": "^0.10.0",
    "sass": "~1.32.0",
    "sass-loader": "^10.0.0",
    "vue-cli-plugin-vuetify": "~2.4.1",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.7.0"
  },
  "watch": {
    "autobarrel": {
      "patterns": [
        "src"
      ],
      "extensions": "js,jsx,ts,tsx,vue",
      "ignore": [
        "**/index.js",
        "dist",
        "node_modules",
        "src/assets",
        "src/styles",
        "src/locales"
      ]
    }
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
